:isRequiredBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :requiresSkill ;
              rdfs:domain :Skill ;
              rdfs:range :Request ;
              rdfs:comment "The skill associated with a Timebank request." ;
              rdfs:isDefinedBy <https://w3id.org/timebank> ;
              rdfs:label "is required by" .


