#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/timebank#accepts
:accepts rdf:type owl:ObjectProperty ;
         owl:inverseOf :isAcceptedBy ;
         rdfs:domain :User ;
         rdfs:range :Request ;
         rdfs:comment "Indicates that a user who is willing to help accepted the request." ;
         rdfs:isDefinedBy <https://w3id.org/timebank> ;
         rdfs:label "accepts" .


###  https://w3id.org/timebank#endsRepeatingOn
:endsRepeatingOn rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isRepeatingEndpointOf ;
                 rdfs:domain <http://www.w3.org/2006/time#ProperInterval> ;
                 rdfs:range <http://www.w3.org/2006/time#Instant> ;
                 rdfs:comment "Indicates that a time-interval is repeating until this time and date." ;
                 rdfs:isDefinedBy <https://w3id.org/timebank> ;
                 rdfs:label "ends repeating on" .


###  https://w3id.org/timebank#excludesUserByLimitation
:excludesUserByLimitation rdf:type owl:ObjectProperty ;
                          owl:inverseOf :excludesUserFrom ;
                          rdfs:domain :Request ;
                          rdfs:range :Limitation ;
                          rdfs:comment "This property is used to describe that a request exludes helpers having a certain limitation." ;
                          rdfs:isDefinedBy <https://w3id.org/timebank> ;
                          rdfs:label "excludes user by limitation" .


###  https://w3id.org/timebank#excludesUserFrom
:excludesUserFrom rdf:type owl:ObjectProperty ;
                  rdfs:domain :Limitation ;
                  rdfs:range :Request ;
                  rdfs:comment "This property is used to describe that a certain limitation excludes certain helpers from the request." ;
                  rdfs:isDefinedBy <https://w3id.org/timebank> ;
                  rdfs:label "excludes user from" .


###  https://w3id.org/timebank#hasAccreditedDuration
:hasAccreditedDuration rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isAccreditedTo ;
                       rdfs:domain :Request ;
                       rdfs:range <http://www.w3.org/2006/time#DurationDescription> ;
                       rdfs:comment "The accredited duration of the request. This is the amount of time which will be transferred from the originator the the assigned helper of the request. Do not use this property to indicate the estimated duration of the task. Use hasExpectedDuration instead." ;
                       rdfs:isDefinedBy <https://w3id.org/timebank> ;
                       rdfs:label "has accredited duration" .


###  https://w3id.org/timebank#hasAddress
:hasAddress rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAddressOf ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( <http://xmlns.com/foaf/0.1/Person>
                                        :Request
                                      )
                        ] ;
            rdfs:range <http://www.w3.org/2006/vcard/ns#VCard> ;
            rdfs:comment "Indicates the address of a person or request. The address is specified as a vcard." ;
            rdfs:isDefinedBy <https://w3id.org/timebank> ;
            rdfs:label "has address" .


###  https://w3id.org/timebank#hasEnvironment
:hasEnvironment rdf:type owl:ObjectProperty ;
                owl:inverseOf :isEnvironmentOf ;
                rdfs:domain :Request ;
                rdfs:range :Environment ;
                rdfs:comment "The environment of a user or request. The environment of a request describes specific environmental factors of a request." ;
                rdfs:isDefinedBy <https://w3id.org/timebank> ;
                rdfs:label "has environment" .


###  https://w3id.org/timebank#hasExpectedDuration
:hasExpectedDuration rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isExpectedDurationOf ;
                     rdfs:domain :Request ;
                     rdfs:range <http://www.w3.org/2006/time#DurationDescription> ;
                     rdfs:comment "The expected duration indicates the duraction of the request estimated by the originator of the request. Do not use this property to assert the actual time credits associated to this request. Use hasAccreditedDuration instead." ;
                     rdfs:isDefinedBy <https://w3id.org/timebank> ;
                     rdfs:label "has expected duration" .


###  https://w3id.org/timebank#hasLimitation
:hasLimitation rdf:type owl:ObjectProperty ;
               owl:inverseOf :isLimitationOf ;
               rdfs:domain :User ;
               rdfs:range :Limitation ;
               rdfs:comment "Indicates that a Timebank user suffers from a certain limitation." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "has limitation" .


###  https://w3id.org/timebank#hasOriginator
:hasOriginator rdf:type owl:ObjectProperty ;
               owl:inverseOf :isOriginatorOf ;
               rdfs:domain :Request ;
               rdfs:range :User ;
               rdfs:comment "Indicates that the request has the specific originator." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "has originator" .


###  https://w3id.org/timebank#hasSeverity
:hasSeverity rdf:type owl:ObjectProperty ;
             owl:inverseOf :isSeverityOf ;
             rdfs:domain :Limitation ;
             rdfs:range :Level ;
             rdfs:comment "Indicates the severity of a limitation." ;
             rdfs:isDefinedBy <https://w3id.org/timebank> ;
             rdfs:label "has severity" .


###  https://w3id.org/timebank#hasSkill
:hasSkill rdf:type owl:ObjectProperty ;
          owl:inverseOf :isSkillOf ;
          rdfs:domain :User ;
          rdfs:range :Skill ;
          rdfs:comment "Describes the Skill of a certain Timebank user." ;
          rdfs:isDefinedBy <https://w3id.org/timebank> ;
          rdfs:label "has skill" .


###  https://w3id.org/timebank#hasSkillLevel
:hasSkillLevel rdf:type owl:ObjectProperty ;
               owl:inverseOf :isSkillLevelOf ;
               rdfs:domain :Skill ;
               rdfs:range :Level ;
               rdfs:comment "Describes the proficiency level of a skill." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "has skill level" .


###  https://w3id.org/timebank#hasStatus
:hasStatus rdf:type owl:ObjectProperty ;
           owl:inverseOf :isStatusOf ;
           rdfs:domain :Request ;
           rdfs:range :Status ;
           rdfs:comment "Every request can have at most one status assigned to it." ;
           rdfs:isDefinedBy <https://w3id.org/timebank> ;
           rdfs:label "has status" .


###  https://w3id.org/timebank#hasTimeBalance
:hasTimeBalance rdf:type owl:ObjectProperty ;
                owl:inverseOf :isTimeBalanceOf ;
                rdfs:domain :User ;
                rdfs:range <http://www.w3.org/2006/time#DurationDescription> ;
                rdfs:comment "Indicates the current time-balance of a Timebank user." ;
                rdfs:isDefinedBy <https://w3id.org/timebank> ;
                rdfs:label "has time-balance" .


###  https://w3id.org/timebank#hasTimeWindow
:hasTimeWindow rdf:type owl:ObjectProperty ;
               owl:inverseOf :isTimeWindowOf ;
               rdfs:domain :Request ;
               rdfs:range <http://www.w3.org/2006/time#ProperInterval> ;
               rdfs:comment "Indicates the time window in which the request can/should be completed." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "has time window" .


###  https://w3id.org/timebank#isAcceptanceDateOf
:isAcceptanceDateOf rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isAcceptedOn ;
                    rdfs:domain <http://www.w3.org/2006/time#Instant> ;
                    rdfs:range :Request ;
                    rdfs:comment "Indicates the time and date when the Timebank request was accepted by the assigned helper." ;
                    rdfs:isDefinedBy <https://w3id.org/timebank> ;
                    rdfs:label "is acceptance date of" .


###  https://w3id.org/timebank#isAcceptedBy
:isAcceptedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :Request ;
              rdfs:range :User ;
              rdfs:comment "Indicates that a request is accepted by a user who is willing to help completing the request." ;
              rdfs:isDefinedBy <https://w3id.org/timebank> ;
              rdfs:label "is accepted by" .


###  https://w3id.org/timebank#isAcceptedOn
:isAcceptedOn rdf:type owl:ObjectProperty ;
              rdfs:domain :Request ;
              rdfs:range <http://www.w3.org/2006/time#Instant> ;
              rdfs:comment "Indicates the time and date when the Timebank request was accepted by the assigned helper." ;
              rdfs:isDefinedBy <https://w3id.org/timebank> ;
              rdfs:label "is accepted on" .


###  https://w3id.org/timebank#isAccreditedTo
:isAccreditedTo rdf:type owl:ObjectProperty ;
                rdfs:domain <http://www.w3.org/2006/time#DurationDescription> ;
                rdfs:range :Request ;
                rdfs:comment "The accredited duration of the request. This is the amount of time which will be transferred from the originator the the assigned helper of the request. Do not use this property to indicate the estimated duration of the task. Use hasExpectedDuration instead." ;
                rdfs:isDefinedBy <https://w3id.org/timebank> ;
                rdfs:label "is accredited to" .


###  https://w3id.org/timebank#isAddressOf
:isAddressOf rdf:type owl:ObjectProperty ;
             rdfs:domain <http://www.w3.org/2006/vcard/ns#VCard> ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( <http://xmlns.com/foaf/0.1/Person>
                                        :Request
                                      )
                        ] ;
             rdfs:comment "Indicates the address of a person or request. The address is specified as a vcard." ;
             rdfs:isDefinedBy <https://w3id.org/timebank> ;
             rdfs:label "is address of" .


###  https://w3id.org/timebank#isAvailabilityOf
:isAvailabilityOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isAvailableOn ;
                  rdfs:domain <http://www.w3.org/2006/time#ProperInterval> ;
                  rdfs:range :User ;
                  rdfs:comment "Indicates the availability of a Timebank user to act as a helper for a Timebank request." ;
                  rdfs:isDefinedBy <https://w3id.org/timebank> ;
                  rdfs:label "is availability of" .


###  https://w3id.org/timebank#isAvailableOn
:isAvailableOn rdf:type owl:ObjectProperty ;
               rdfs:domain :User ;
               rdfs:range <http://www.w3.org/2006/time#ProperInterval> ;
               rdfs:comment "Indicates the availability of a Timebank user to act as a helper for a Timebank request." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "is available on" .


###  https://w3id.org/timebank#isCancellationDateOf
:isCancellationDateOf rdf:type owl:ObjectProperty ;
                      owl:inverseOf :isCancelledOn ;
                      rdfs:domain <http://www.w3.org/2006/time#Instant> ;
                      rdfs:range :Request ;
                      rdfs:comment "Indicates the time and date when the Timebank request was cancelled." ;
                      rdfs:isDefinedBy <https://w3id.org/timebank> ;
                      rdfs:label "is cancellation date of" .


###  https://w3id.org/timebank#isCancelledOn
:isCancelledOn rdf:type owl:ObjectProperty ;
               rdfs:domain :Request ;
               rdfs:range <http://www.w3.org/2006/time#Instant> ;
               rdfs:comment "Indicates the time and date when the Timebank request was cancelled." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "is cancelled on" .


###  https://w3id.org/timebank#isCompletedOn
:isCompletedOn rdf:type owl:ObjectProperty ;
               owl:inverseOf :isCompletionDateOf ;
               rdfs:domain :Request ;
               rdfs:range <http://www.w3.org/2006/time#Instant> ;
               rdfs:comment "Indicates the time and date of the completion of a Timebank request." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "is completed on" .


###  https://w3id.org/timebank#isCompletionDateOf
:isCompletionDateOf rdf:type owl:ObjectProperty ;
                    rdfs:domain <http://www.w3.org/2006/time#Instant> ;
                    rdfs:range :Request ;
                    rdfs:comment "Indicates the time and date of the completion of a Timebank request." ;
                    rdfs:isDefinedBy <https://w3id.org/timebank> ;
                    rdfs:label "is completion date of" .


###  https://w3id.org/timebank#isEnvironmentOf
:isEnvironmentOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :Environment ;
                 rdfs:range :Request ;
                 rdfs:comment "The environment of a user or request. The environment of a request describes specific environmental factors of a request. The environment of a user can be used to infer environmental factors of a request if the user is the originator of the request. However, not all environmental factors of a user might apply to its request, especially if the location of the request differs from the users location." ;
                 rdfs:isDefinedBy <https://w3id.org/timebank> ;
                 rdfs:label "is environment of" .


###  https://w3id.org/timebank#isExpectedDurationOf
:isExpectedDurationOf rdf:type owl:ObjectProperty ;
                      rdfs:domain <http://www.w3.org/2006/time#DurationDescription> ;
                      rdfs:range :Request ;
                      rdfs:comment "The expected duration indicates the duraction of the request estimated by the originator of the request. Do not use this property to assert the actual time credits associated to this request. Use hasAccreditedDuration instead." ;
                      rdfs:isDefinedBy <https://w3id.org/timebank> ;
                      rdfs:label "is expected duration of" .


###  https://w3id.org/timebank#isLimitationOf
:isLimitationOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Limitation ;
                rdfs:range :User ;
                rdfs:comment "Indicates that a certain limitation applies to the Timebank user." ;
                rdfs:isDefinedBy <https://w3id.org/timebank> ;
                rdfs:label "is limitation of" .


###  https://w3id.org/timebank#isOriginatorOf
:isOriginatorOf rdf:type owl:ObjectProperty ;
                rdfs:domain :User ;
                rdfs:range :Request ;
                rdfs:comment "Indicates that the user is the originator of the request." ;
                rdfs:isDefinedBy <https://w3id.org/timebank> ;
                rdfs:label "is originator of" .


###  https://w3id.org/timebank#isPublicationDateOf
:isPublicationDateOf rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isPublishedOn ;
                     rdfs:domain <http://www.w3.org/2006/time#Instant> ;
                     rdfs:range :Request ;
                     rdfs:comment "Indicates the time and date of the publication of a Timebank request." ;
                     rdfs:isDefinedBy <https://w3id.org/timebank> ;
                     rdfs:label "is publication date of" .


###  https://w3id.org/timebank#isPublishedOn
:isPublishedOn rdf:type owl:ObjectProperty ;
               rdfs:domain :Request ;
               rdfs:range <http://www.w3.org/2006/time#Instant> ;
               rdfs:comment "Indicates the time and date of the publication of a Timebank request." ;
               rdfs:isDefinedBy <https://w3id.org/timebank> ;
               rdfs:label "is published on" .


###  https://w3id.org/timebank#isRepeatingEndpointOf
:isRepeatingEndpointOf rdf:type owl:ObjectProperty ;
                       rdfs:domain <http://www.w3.org/2006/time#Instant> ;
                       rdfs:range <http://www.w3.org/2006/time#ProperInterval> ;
                       rdfs:comment "Indicates that a specific time and date marks the end of a repetition of a time-interval." ;
                       rdfs:isDefinedBy <https://w3id.org/timebank> ;
